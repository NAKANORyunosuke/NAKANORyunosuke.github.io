---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- comment -%}
    Google Search Console submission URL should be:
    https://nakanoryunosuke.github.io/sitemap.xml
    (no trailing slash)
  {%- endcomment -%}

  {%- comment -%} Pages {%- endcomment -%}
  {%- for doc in site.pages -%}
    {%- assign p = doc.path | downcase -%}
    {%- assign ext4 = p | slice: -4, 4 -%}
    {%- if doc.sitemap != false and doc.published != false -%}
      {%- unless p contains 'assets/' or p contains 'design/' or p contains 'node_modules/' or p contains '.md.sample' -%}
        {%- unless ext4 == '.css' or ext4 == '.xml' or ext4 == '.txt' -%}
          <url>
            <loc>{{ doc.url | absolute_url }}</loc>
            {%- assign page_lastmod = doc.last_modified_at | default: doc.date -%}
            {%- if page_lastmod -%}
            <lastmod>{{ page_lastmod | date_to_xmlschema }}</lastmod>
            {%- endif -%}
          </url>
        {%- endunless -%}
      {%- endunless -%}
    {%- endif -%}
  {%- endfor -%}

  {%- comment -%} Posts {%- endcomment -%}
  {%- for doc in site.posts -%}
    {%- if doc.sitemap != false and doc.published != false -%}
      <url>
        <loc>{{ doc.url | absolute_url }}</loc>
        {%- assign post_lastmod = doc.last_modified_at | default: doc.date -%}
        {%- if post_lastmod -%}
        <lastmod>{{ post_lastmod | date_to_xmlschema }}</lastmod>
        {%- endif -%}
      </url>
    {%- endif -%}
  {%- endfor -%}

  {%- comment -%} Other collections (e.g., projects) {%- endcomment -%}
  {%- for collection in site.collections -%}
    {%- unless collection.label == 'posts' or collection.label == 'pages' -%}
      {%- for doc in collection.docs -%}
        {%- assign p = doc.path | downcase -%}
        {%- assign ext4 = p | slice: -4, 4 -%}
        {%- if doc.sitemap != false and doc.published != false -%}
          {%- unless p contains 'assets/' or p contains 'design/' or p contains 'node_modules/' or p contains '.md.sample' -%}
            {%- unless ext4 == '.css' or ext4 == '.xml' or ext4 == '.txt' -%}
              <url>
                <loc>{{ doc.url | absolute_url }}</loc>
                {%- assign doc_lastmod = doc.last_modified_at | default: doc.date -%}
                {%- if doc_lastmod -%}
                <lastmod>{{ doc_lastmod | date_to_xmlschema }}</lastmod>
                {%- endif -%}
              </url>
            {%- endunless -%}
          {%- endunless -%}
        {%- endif -%}
      {%- endfor -%}
    {%- endunless -%}
  {%- endfor -%}
</urlset>
